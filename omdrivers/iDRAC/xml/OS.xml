    <REGISTRY_ENTRIES>
      <Attribute>
        <AttributeName>NetworkSharePath</AttributeName>
        <DisplayName>Share Path where ISO Image is present</DisplayName>
        <AttributeType>String</AttributeType>
        <HelpText>Specify whether SPMA or FPMA addressing will be used for FCoE transactions.</HelpText>
        <GroupName>ShareDetails</GroupName>
        <GroupDisplayName>Share Details</GroupDisplayName>
        <DisplayOrder>100</DisplayOrder>
        <IsReadOnly>false</IsReadOnly>
        <ExpertiseLevel>Typical</ExpertiseLevel>
        <Modifiers>
          <BrowserSuppressed>Yes</BrowserSuppressed>
          <BrowserRead>No</BrowserRead>
          <BrowserWrite>No</BrowserWrite>
          <ProgrammaticRead>Yes</ProgrammaticRead>
          <ProgrammaticWrite>No</ProgrammaticWrite>
        </Modifiers>
      </Attribute>
      <Attribute>
        <AttributeName>Ipv4Netmask</AttributeName>
        <DisplayName>IPv4 Netmask</DisplayName>
        <AttributeType>String</AttributeType>
        <HelpText>Specify the IPv4 IP address of the default Gateway used by the iSCSI initiator.</HelpText>
        <GroupName>NetworkConfiguration</GroupName>
        <GroupDisplayName>Network Configuration</GroupDisplayName>
        <DisplayOrder>1104</DisplayOrder>
        <IsReadOnly>false</IsReadOnly>
        <ExpertiseLevel>Typical</ExpertiseLevel>
        <PoolType>IPv4</PoolType>
	<DefaultValue>255.255.255.0</DefaultValue>
        <Modifiers>
          <BrowserSuppressed>No</BrowserSuppressed>
          <BrowserRead>Yes</BrowserRead>
          <BrowserWrite>Yes</BrowserWrite>
          <ProgrammaticRead>Yes</ProgrammaticRead>
          <ProgrammaticWrite>Yes</ProgrammaticWrite>
          <RegEx>^([0-9]{1,3}\.){3}[0-9]{1,3}$</RegEx>
        </Modifiers>
        <Min>7</Min>
        <Max>15</Max>
      </Attribute>
      <Attribute>
        <AttributeName>Ipv4Gateway</AttributeName>
